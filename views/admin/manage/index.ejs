<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>dashboard</title>
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.css" />
    <link rel="stylesheet" href="/css/basic.css" />
    <script
      type="text/javascript"
      src="/bootstrap/js/jquery-1.10.1.js"
    ></script>
    <script type="text/javascript" src="/js/base.js"></script>
  </head>
  <body>
    <nav class="navbar navbar-inverse" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header"></div>
        <div class="collapse navbar-collapse" id="example-navbar-collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a>Welcoome, Admin</a></li>
            <li><a href="#">Logout</a></li>
          </ul>
        </div>
      </div>
    </nav>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-2">
          <ul class="aside">
            <li>
              <h4>Manage User</h4>
              <ul>
                <li class="list-group-item"><a href="/">Users</a></li>
                <li class="list-group-item"><a href="/add">Add User</a></li>
              </ul>
            </li>
            <li>
              <h4>Category</h4>
              <ul>
                <li class="list-group-item"><a href="/">Categories</a></li>
                <li class="list-group-item"><a href="/add">Add Category</a></li>
              </ul>
            </li>
            <li>
              <h4>Product</h4>
              <ul>
                <li class="list-group-item"><a href="/">Products</a></li>
                <li class="list-group-item"><a href="/add">Add Product</a></li>
              </ul>
            </li>
            <li>
              <h4>Carousel</h4>
              <ul>
                <li class="list-group-item"><a href="/">Carousel</a></li>
                <li class="list-group-item">
                  <a href="/add">Add Carousel Product</a>
                </li>
              </ul>
            </li>
          </ul>
        </div>
        <div class="col-sm-10">
          <div class="panel panel-default">
            <div class="panel-heading">Search</div>
            <div class="panel-body">
              <form role="form" class="form-inline">
                <div class="form-group">
                  <label for="name">Name</label>
                  <input
                    type="text"
                    class="form-control"
                    id="name"
                    placeholder="Search by name.."
                  />
                </div>
                <div class="form-group">
                  <button type="submit" class="btn btn-default">Search</button>
                </div>
              </form>
            </div>
          </div>
          <div class="table-responsive">
            <table class="table table-bordered">
              <thead>
                <tr class="th">
                  <th>SKU</th>
                  <th>ICON</th>
                  <th>NAME</th>
                  <th>PRICE</th>
                  <th>SHIPPING FEE</th>
                  <th class="text-center">Confirm</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>1</td>
                  <td>icon1</td>
                  <td>backpack</td>
                  <td>$20</td>
                  <td>$10</td>
                  <td class="text-center">Edit Delete</td>
                </tr>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
